{
  "manifest_version": 2,

  "name": "covfefe",
  "description": "adds more covfefe",
  "version": "1.0",

  "browser_action": {
    "default_icon": "covfefe.gif",
    "default_popup": "covfefe.html"
  },

  "content_scripts":
  [
    {
      "matches": ["*://*/*"],
      "js": ["covfefe.js"],
      "run_at": "document_end"
    }
  ]
}
